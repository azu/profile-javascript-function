{
  "name": "profile-javascript-function",
  "version": "1.0.0",
  "description": "Profile JavaScript function tool.",
  "keywords": [
    "browser",
    "javascript",
    "performance"
  ],
  "homepage": "https://github.com/azu/profile-javascript-function",
  "bugs": {
    "url": "https://github.com/azu/profile-javascript-function/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/profile-javascript-function.js",
  "types": "lib/profile-javascript-function.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/profile-javascript-function.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "precommit": "lint-staged",
    "postcommit": "git reset",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "watch": "tsc -p . --watch"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.0",
    "cross-env": "^5.1.3",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "mocha": "^5.0.0",
    "prettier": "^1.10.2",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4
  },
  "dependencies": {
    "@babel/generator": "^7.0.0-beta.39",
    "@babel/template": "^7.0.0-beta.39",
    "@babel/traverse": "^7.0.0-beta.39",
    "babylon": "^6.18.0"
  }
}
